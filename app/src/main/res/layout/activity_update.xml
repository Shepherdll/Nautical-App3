<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.swampass.nauticalapp.UpdateProfile"
    android:background="@drawable/pro_background">

    <Button
        android:id="@+id/btn_change2"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:text="Change"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/languages_btn"
        app:layout_constraintRight_toRightOf="@+id/languages_btn"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="79dp"
        app:layout_constraintLeft_toLeftOf="@+id/languages_btn" />

    <include
        android:id="@+id/include3"
        layout="@layout/toolbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.4" />

    <EditText
        android:id="@+id/lang_txt"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:ems="10"
        android:hint="Enter Languages"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="19dp"
        android:layout_marginTop="47dp"
        app:layout_constraintTop_toBottomOf="@+id/int_txt"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/bio_btn"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:text="Change"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/btn_change2"
        app:layout_constraintRight_toRightOf="@+id/btn_change2"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="99dp"
        app:layout_constraintLeft_toLeftOf="@+id/btn_change2"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/languages_btn"
        android:layout_width="138dp"
        android:layout_height="0dp"
        android:text="Change"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/lang_txt"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/dick_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="18dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="28dp" />

    <android.support.design.widget.TextInputLayout
        android:id="@+id/bio_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/include3"
        android:layout_marginTop="63dp"
        app:layout_constraintTop_toBottomOf="@+id/include3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/include3">

        <EditText
            android:id="@+id/editText"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:hint="Interests"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="117dp" />
    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:id="@+id/int_txt"
        android:layout_width="0dp"
        android:layout_height="84dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/dick_btn"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="246dp"
        app:layout_constraintTop_toBottomOf="@+id/include3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="246dp"
        app:layout_constraintLeft_toLeftOf="parent">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="hint" />
    </android.support.design.widget.TextInputLayout>


</android.support.constraint.ConstraintLayout>
